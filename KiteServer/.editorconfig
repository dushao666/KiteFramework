[*.cs]

# CS8604: 引用类型参数可能为 null。
dotnet_diagnostic.CS8604.severity = silent

# CS8618: 在退出构造函数时，不可为 null 的字段必须包含非 null 值。请考虑声明为可以为 null。
dotnet_diagnostic.CS8618.severity = silent

# CS8603: 可能返回 null 引用。
dotnet_diagnostic.CS8603.severity = silent

# CS0649: 从未对字段“TouristTeam._reservations”赋值，字段将一直保持其默认值 null
dotnet_diagnostic.CS0649.severity = silent

# CS8625: 无法将 null 字面量转换为非 null 的引用类型。
dotnet_diagnostic.CS8625.severity = silent

# CS8634: 类型不能用作泛型类型或方法中的类型参数。类型参数的为 Null 性与 “class” 约束不匹配。
dotnet_diagnostic.CS8634.severity = silent

# CS8602: 解引用可能出现空引用。
dotnet_diagnostic.CS8602.severity = silent

# CS8622: 参数类型中引用类型的为 Null 性与目标委托不匹配(可能是由于为 Null 性特性)。
dotnet_diagnostic.CS8622.severity = silent

# CS8601: 引用类型赋值可能为 null。
dotnet_diagnostic.CS8601.severity = silent

# CA1305: Specify IFormatProvider
dotnet_diagnostic.CA1305.severity = silent

# CS0618: 类型或成员已过时
dotnet_diagnostic.CS0618.severity = silent

# IDE0058: 永远不会使用表达式值
dotnet_diagnostic.IDE0058.severity = silent

# IDE0011: 添加大括号
dotnet_diagnostic.IDE0011.severity = silent

# CS8629: 可为 null 的值类型可为 null。
dotnet_diagnostic.CS8629.severity = silent

# CS8600: 将 null 字面量或可能为 null 的值转换为非 null 类型。
dotnet_diagnostic.CS8600.severity = silent

# IDE0008: 使用显式类型
dotnet_diagnostic.IDE0008.severity = silent

