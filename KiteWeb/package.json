{
  "name": "easypan",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node --require ./build/prevent-sass-warnings.cjs ./node_modules/vite/bin/vite.js",
    "build": "node --require ./build/prevent-sass-warnings.cjs ./node_modules/vite/bin/vite.js build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "build:prod": "node --require ./build/prevent-sass-warnings.cjs ./node_modules/vite/bin/vite.js build --mode production",
